#include <iostream>
using namespace std;

int main(){
    //membuat program goto
    char jump = 'g' ;
    
    cout << "masukan huruf e untuk melopat" << endl;
    cout << "masukan huruf f untuk tidak melompat" << endl;
    cin >> jump;
    
    cout << "X" << endl;
    if ( jump == 'a' ) goto lompat;
    cout << "Y" << endl;
    lompat:
    cout << "Z" << endl;
    
    return 0;
}
